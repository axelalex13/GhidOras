<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/statistici">


    <TextView
        android:id="@+id/totalOrganizator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="36dp"
        android:textSize="16sp"
        android:layout_marginRight="20dp"
        android:text="Evenimente la care ai fost organizator: " />

    <TextView
        android:id="@+id/locatii"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginRight="20dp"
        android:text="TextView"
        android:textSize="16sp"
        android:layout_below="@+id/totalOrganizator"
        android:layout_alignStart="@+id/totalOrganizator" />


</RelativeLayout>